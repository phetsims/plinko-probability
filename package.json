{
  "name": "plinko-probability",
  "version": "1.2.0-dev.6",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/plinko-probability.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "PLINKO_PROBABILITY",
    "phetLibs": [
      "vibe"
    ],
    "runnable": true,
    "supportedBrands": [
      "phet",
      "adapted-from-phet"
    ],
    "simulation": true,
    "supportsOutputJS": true,
    "published": true,
    "screenNameKeys": [
      "PLINKO_PROBABILITY/screen.intro",
      "PLINKO_PROBABILITY/screen.lab"
    ]
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "overrides": [
      {
        "files": [
          "**/*.ts"
        ],
        "parserOptions": {
          "project": [
            "../plinko-probability/tsconfig.json"
          ]
        }
      }
    ]
  }
}